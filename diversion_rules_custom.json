{
  "rules": [
    {
      "outbound": "block",
      "name": "🛑 广告拦截",
      "switch": true,
      "or": true,
      "rule_set_build_in": [
        "acl:BanAD",
        "geosite:category-ads"
      ]
    },
    {
      "outbound": "block",
      "name": "🍃 应用净化",
      "switch": true,
      "or": true,
      "rule_set_build_in": [
        "acl:BanProgramAD",
        "acl:BanADCompany"
      ]
    },
    {
      "outbound": "block",
      "name": "🛑 恶意软件",
      "switch": true,
      "or": true,
      "rule_set_build_in": [
        "geosite:malware",
        "geoip:malware",
        "geoip:phishing",
        "geosite:phishing",
        "geosite:cryptominers"
      ]
    },
    {
      "outbound": "direct",
      "name": "🍎 苹果服务",
      "switch": true,
      "or": true,
      "rule_set_build_in": [
        "geosite:apple",
        "geosite:apple-ads",
        "geosite:apple-dev",
        "geosite:apple-pki",
        "geosite:apple-update"
      ]
